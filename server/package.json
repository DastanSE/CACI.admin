    "cloudinary": "^1.9.1",
    "concurrently": "^3.5.0",
    "cookie-session": "^1.3.2",
    "cors": "^2.8.4",
    "express": "^4.16.1",
    "mongoose": "^4.12.1",
    "morgan": "^1.9.0",
    "nodemon": "^1.12.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DastanSE/CACI.admin.git"
  },
